<settings>

    <setting propertyKey="@Properties.Language" title="@Strings.LanguageTitle">
        <settingConfig type="list">
            <listEntry value="1">@Strings.Bayern</listEntry>
            <listEntry value="2">@Strings.Kaernten</listEntry>
        </settingConfig>
    </setting>

	<setting propertyKey="@Properties.Font" title="@Strings.FontTitle">
		<settingConfig type="list">
			<listEntry value="1">@Strings.Font1</listEntry>
			<listEntry value="2">@Strings.Font2</listEntry>
			<listEntry value="3">@Strings.Font3</listEntry>
		</settingConfig>
	</setting>

    <setting propertyKey="@Properties.HighlightColor" title="@Strings.HighlightColorTitle">
        <settingConfig type="list">
            <listEntry value="1">@Strings.ColorRed</listEntry>
            <listEntry value="2">@Strings.ColorDKRed</listEntry>
            <listEntry value="3">@Strings.ColorBlue</listEntry>
            <listEntry value="4">@Strings.ColorDKBlue</listEntry>
            <listEntry value="5">@Strings.ColorGreen</listEntry>
            <listEntry value="6">@Strings.ColorDKGreen</listEntry>
            <listEntry value="7">@Strings.ColorOrange</listEntry>
            <listEntry value="8">@Strings.ColorLightGray</listEntry>
            <listEntry value="9">@Strings.ColorDarkGray</listEntry>
            <listEntry value="10">@Strings.ColorWhite</listEntry>
        </settingConfig>
    </setting>
</settings>
